&MESH
  mesh_file = 'disk+mold.g'
  coordinate_scale_factor = 0.01
/

&OUTPUTS
  Output_T  = 0.0, 0.1
  Output_Dt = 0.1
/

&PHYSICS
  materials = 'water', 'copper', 'VOID'
  flow = .true.
  body_force_density = 4.9, -8.5, 0.0
  heat_transport = .true.
/

&DIFFUSION_SOLVER
  stepping_method    = 'Non-adaptive BDF1'
  residual_atol      = 0.0
  residual_rtol      = 1.0e-6
  !abs_temp_tol       = 0.0
  !rel_temp_tol       = 1.0e-5
  !abs_enthalpy_tol   = 0.0
  !rel_enthalpy_tol   = 1.0e-5
  max_nlk_itr        = 20
  nlk_tol            = 0.05
  nlk_preconditioner = 'hypre_amg'
  verbose_stepping   = .true.
  void_temperature   = 300.0
/

&DS_SOURCE
  equation = 'temperature'
  cell_set_ids = 3
  source_constant = 3.5e8
/
  

&NUMERICS
  dt_init = 1.0e-3
  dt_grow = 1.05
  dt_min  = 1.0e-6
  cutvof                     = 1.0e-8
  discrete_ops_type          = 'ortho' 
/

&FLOW
  inviscid = .false.
  viscous_implicitness = 1.0
  viscous_number = 0
  vol_track_subcycles = 1
  courant_number = 0.2
/

&FLOW_VISCOUS_SOLVER
  rel_tol = 1.0e-8
  abs_tol = 0.0
  max_ds_iter = 50
  max_amg_iter = 5
  krylov_method = 'cg'
/ 

&FLOW_PRESSURE_SOLVER
  rel_tol = 0.0
  abs_tol = 1.0e-12
  max_ds_iter = 50
  max_amg_iter = 50
  krylov_method = 'cg'
  gmres_krylov_dim = 5
/

Fluid-filled region in the lower part of the domain
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 1
  material_name = 'water'
  velocity        = 3*0.0
  temperature     = 300.0
/

Void region in the upper part of the domain
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 2
  material_name = 'VOID'
  temperature     = 300.0
/

Surrounding mold region
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 3
  material_name = 'copper'
  temperature     = 300.0
/

&THERMAL_BC
  name = 'symmetry planes'
  face_set_ids = 1
  type = 'flux'
  flux = 0.0
/

&THERMAL_BC
  name = 'cirumference'
  face_set_ids = 2
  type = 'flux'
  flux = 0.0
/

&FLOW_BC
  name = 'walls'
  face_set_ids = 12
  type = 'no-slip'
/

&MATERIAL
  name = 'water'
  is_fluid = T
  density = 1.0e3
  specific_heat = 4.1e3
  conductivity = 61.0 !0.61
  viscosity = 8.3e-4
/

&MATERIAL
  name = 'copper'
  density = 8.9e3
  specific_heat = 390.0
  conductivity = 390.0
/
