&MESH
  mesh_file = 'disk+mold.g'
  coordinate_scale_factor = 0.01
/

&OUTPUTS
  Output_T  = 0.0, 0.1
  Output_Dt = 0.1
/

&PHYSICS
  flow = .true.
  body_force_density = 4.9, -8.5, 0.0
  heat_transport = .true.
/

&DIFFUSION_SOLVER
  stepping_method    = 'Non-adaptive BDF1'
  residual_atol      = 0.0
  residual_rtol      = 1.0e-6
  !abs_temp_tol       = 0.0
  !rel_temp_tol       = 1.0e-5
  !abs_enthalpy_tol   = 0.0
  !rel_enthalpy_tol   = 1.0e-5
  max_nlk_itr        = 20
  nlk_tol            = 0.05
  nlk_preconditioner = 'hypre_amg'
  verbose_stepping   = .true.
/

&DS_SOURCE
  equation = 'temperature'
  cell_set_ids = 3
  source_constant = 3.5e8
/
  

&NUMERICS
  dt_init = 1.0e-3
  dt_grow = 1.05
  dt_min  = 1.0e-6
  cutvof                     = 1.0e-8
  discrete_ops_type          = 'ortho' 
/

&FLOW
  inviscid = .false.
  viscous_implicitness = 1.0
  viscous_number = 100.0
  vol_track_subcycles = 1
  courant_number = 0.2
/

&FLOW_VISCOUS_SOLVER
  rel_tol = 1.0e-8
  abs_tol = 0.0
  max_ds_iter = 50
  max_amg_iter = 5
  krylov_method = 'cg'
/ 

&FLOW_PRESSURE_SOLVER
  rel_tol = 0.0
  abs_tol = 1.0e-12
  max_ds_iter = 50
  max_amg_iter = 50
  krylov_method = 'gmres'
  gmres_krylov_dim = 5
/

Fluid-filled region in the lower part of the domain
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 1
  material_number = 1
  velocity        = 3*0.0
  temperature     = 300.0
/

Void region in the upper part of the domain
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 2
  material_number = 2
  temperature     = 300.0
/

Surrounding mold region
&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 3
  material_number = 3
  temperature     = 300.0
/

&DS_BOUNDARY_CONDITION
  name = 'symmetry planes'
  variable = 'temperature'
  condition = 'flux'
  face_set_ids = 1
  data_constant = 0.0
/

&DS_BOUNDARY_CONDITION
  name = 'cirumference'
  variable = 'temperature'
  condition = 'flux'
  face_set_ids = 2
  data_constant = 0.0
/

&FLOW_BC
  name = 'walls'
  face_set_ids = 12
  condition = 'no slip'
/

&MATERIAL
  Material_Name   = 'water'
  Material_Number = 1
  priority        = 1
  Density         = 1.0e3
/

&PHASE
  name = 'water'
  property_name(1) = 'density',       property_constant(1) = 1.0e3
  property_name(2) = 'specific heat', property_constant(2) = 4.1e3
  property_name(3) = 'conductivity',  property_constant(3) = 61.0 !0.61
  property_name(4) = 'viscosity',     property_constant(4) = 8.3e-4
/

&MATERIAL_SYSTEM
  name = 'water'
  phases = 'water'
/

&MATERIAL
  Material_Name = 'void'
  Material_Number = 2
  Material_Feature = 'background'
  priority = 2
  Density = 0.0
  void_temperature = 300.0
/

&MATERIAL
  Material_Name   = 'copper'
  Material_Number = 3
  priority        = 3
  Immobile        = .true.
  Density         = 8.9e3
/

&PHASE
  name = 'copper'
  property_name(1) = 'density',       property_constant(1) = 8.9e3
  property_name(2) = 'specific heat', property_constant(2) = 390.0
  property_name(3) = 'conductivity',  property_constant(3) = 390.0
/

&MATERIAL_SYSTEM
  name = 'copper'
  phases = 'copper'
/
