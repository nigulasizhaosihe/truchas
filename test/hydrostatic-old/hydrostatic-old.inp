Simple Multimaterial Hydrostatic Test -- Should be steady!

Mesh should be an integer multiple of Ncell = 12, 1, 10 in i,j,k space


&MESH

    Mesh_File = 'mesh.exo'

/

&OUTPUTS
    Output_T                    = 0.0, 0.25
    Output_Dt                   = 0.0005
    !Int_Output_Dt_Multiplier    = 0
    !Short_Output_Dt_Multiplier  = 1

/

&LINEAR_SOLVER
    name                            = 'projection'
    method                          = 'fgmres'
    preconditioning_method          = 'ssor'
    convergence_criterion           = 1.0e-11
    relaxation_parameter            = 1.4
    preconditioning_steps           = 2
    maximum_iterations              = 200

/

&LEGACY_FLOW
    inviscid = .true.
    volume_track_interfaces     = .true.
    volume_track_brents_method  = .true.
    volume_track_iter_tol       = 1.0e-12
    projection_linear_solution  = 'projection'
    body_force_face_method      = .true.
    courant_number              = 0.5
/

&NUMERICS
    dt_init                     = 0.0005
    dt_grow                     = 1.
    dt_min                      = 1.0e-05
    dt_max                      = 1.0
    cycle_max                   = 20
    discrete_ops_type           = 'ortho'

/

&PHYSICS
  materials = 'water', 'air', 'solid'
    body_force_density     = 0.0, 0.0, -9.81
    legacy_flow            = .true.
/


&BC
    Surface_Name     = 'conic'
    Conic_X          = 1.0
    Conic_Constant   = 0.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'
/

&BC
    Surface_Name     = 'conic'
    Conic_X          = 1.0
    Conic_Constant   = -1.2
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'
/

&BC
    Surface_Name     = 'conic'
    Conic_Z          = 1.0
    Conic_Constant   = 0.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'
/

&BC
    Surface_Name     = 'conic'
    Conic_Z          = 1.0
    Conic_Constant   = -1.0
    Conic_Tolerance  = 1.0e-6
    BC_Variable      = 'velocity'
    BC_Type          = 'no-slip'
/
 
&BODY
    Material_name    = 'air'
    Surface_Name     = 'box'
    Fill             = 'inside'
    Length           = 1.0000, 0.05, 0.50
    Translation_Pt   = 0.7000, 0.025,0.75
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 273.0
/

&BODY
    Material_name    = 'solid'
    Surface_Name     = 'box'
    Fill             = 'inside'
    Length           = 0.200, 0.050, 1.00
    Translation_Pt   = 0.100, 0.025, 0.50
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 273.0
/

&BODY
    Material_name    = 'water'
    Surface_Name     = 'background'
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 273.0
/

&MATERIAL
    name = 'water'
    is_fluid = T
    density = 1000.0
    specific_heat = 1.0
/

&MATERIAL
    name = 'air'
    is_fluid = T
    density = 1.2
    specific_heat = 1.0
/

&MATERIAL
    name = 'solid'
    density = 3000.0
    specific_heat = 1.0
/

&PARALLEL_PARAMETERS

    Partitioner  = 'automatic', 

/
