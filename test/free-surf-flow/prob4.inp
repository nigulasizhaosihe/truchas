&MESH
  mesh_file = 'mesh2.g'
/

&OUTPUTS
  output_t  = 0.0, 4.0
  output_dt = 0.5
/

A kludge to force hitting the final time exactly
&SIMULATION_CONTROL
  phase_start_times = 4.0
  phase_init_dt_factor = 1.0
/

&PHYSICS
  flow = .true.
/

&NUMERICS
  dt_init = 0.001
  dt_grow = 1.05
  dt_min  = 1.0e-05
  dt_max  = 1.0
/

&FLOW
  inviscid = t
  courant_number = 0.25
  subcycles = 2
  nested_dissection = f
/

&FLOW_CORRECTOR
  rel_tol = 0.0
  abs_tol = 1.0e-10
  max_ds_iter = 50
  max_amg_iter = 50
  krylov_method = "cg"
/

&FLOW_BC
  name = 'outlet'
  condition = 'velocity dirichlet'
  face_set_ids = 1
  data_constant = -1.0, 0.0, 0.0
/

&FLOW_BC
  name = 'inlet'
  condition = 'pressure dirichlet'
  face_set_ids = 2
  data_constant = 0.0
/

&FLOW_BC
  name = 'symmetry'
  condition = 'slip'
  face_set_ids = 3
/
 
Void to the right
&BODY
  surface_name = 'plane'
  axis = 'x'
  translation_pt = 2.0, 0.0, 0.0
  material_number = 2
  velocity = -1.0, 0.0, 0.0
  temperature = 0.0
/

Fluid to the left
&BODY
  surface_name = 'background'
  mesh_material_number = 1
  material_number = 1
  velocity = -1.0, 0.0, 0.0
  temperature = 0.0
/

&MATERIAL
  material_name = 'water'
  material_number = 1
  density = 1.0 ! not void
/

&PHASE
  name = 'water'
  property_name(1) = 'density', property_constant(1) = 1.0
/

&MATERIAL_SYSTEM
  name = 'water'
  phases = 'water'
/

&MATERIAL
  material_name = 'void'
  material_number = 2
  density = 0.0
  material_feature = 'background'
/
