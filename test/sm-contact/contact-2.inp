&MESH
  mesh_file = 'rot-gap-xyz.gen'
  coordinate_scale_factor = 1e-3
  interface_side_sets = 10, 11
/

&OUTPUTS
  output_t = 0.0, 1.0
  output_dt = 1.0
/

&NUMERICS
  dt_init = 1.0
  dt_max  = 1.0
/

&PHYSICS
  materials = 'stuff'
  solid_mechanics = .true.
/

&SOLID_MECHANICS
  preconditioner_method = "ds"
  abs_displ_tol = 1e100
  rel_displ_tol = 1e100
  abs_stress_tol = 1e-8
  maximum_iterations = 1000
/

&MATERIAL
  name = 'stuff'
  density = 1.0
  tm_lame1 = 2e10
  tm_lame2 = 1e10
  tm_ref_density = 1.0
  tm_ref_temp = 1.0
  tm_linear_cte = 0.0
  ref_temp = 1.0
  ref_enthalpy = 0.0
  specific_heat = 1.0
  conductivity = 1.0
/

&BODY
  surface_name = 'from mesh file'
  material_name = 'stuff'
  mesh_material_number = 1
  temperature = 1.0
/

&SOLID_MECHANICS_BC
  name = 'displx'
  face_set_ids = 1
  type = 'displacement-x'
  displacement = 0
/

&SOLID_MECHANICS_BC
  name = 'disply'
  face_set_ids = 1
  type = 'displacement-y'
  displacement = 0
/

&SOLID_MECHANICS_BC
  name = 'displz'
  face_set_ids = 1
  type = 'displacement-z'
  displacement = 0
/

&SOLID_MECHANICS_BC
  name = 'displn'
  face_set_ids = 2
  type = 'displacement-n'
  displacement = -1e-8
/

x&SOLID_MECHANICS_BC
  name = 'tracn'
  face_set_ids = 2
  type = 'traction-n'
  traction = -1e5
/

&SOLID_MECHANICS_BC
  name = 'contact'
  face_set_ids = 10, 11
  type = 'gap-contact'
/
