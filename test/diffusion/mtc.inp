Species diffusion with interfaces
* Generated by CMB 14-Jun-2022  08:33

&MESH
  mesh_file = "mesh.gen"
  interface_side_sets = 10
/

&PHYSICS
  heat_transport = F ! HT problem should be identical to ST problem
  species_transport = T
  number_of_species = 1
  materials = "glass"
/

&DIFFUSION_SOLVER
  abs_enthalpy_tol = 0.0
  rel_enthalpy_tol = 1.0e-4
  abs_temp_tol = 0.0
  rel_temp_tol = 1.0e-4
  abs_conc_tol = 0.0
  rel_conc_tol = 1.0e-4
/

&NUMERICS
  dt_init = 1e-4
  dt_grow = 5.0
  dt_max  = 10.0
  dt_min  = 1e-6
/

&OUTPUTS
  output_t   = 0.0, 1.0
  output_dt  = 1.0 !0.1
/

### MATERIALS ##################################################################

&MATERIAL
  name = "glass"
  density = 1.0
  specific_heat = 1.0
  conductivity  = 1.0
  diffusivity   = 1.0 ! diffusivity of solutal species in "glass"
/

### BOUNDARY CONDITIONS ########################################################

&THERMAL_BC
  name = "sides/right end"
  face_set_ids = 1, 3
  type = "flux"
  flux = 0.0
/

&THERMAL_BC
  name = "left end"
  face_set_ids = 2
  type = "htc"
  htc = 1.0
  ambient_temp = 2.0
/

&SPECIES_BC
  name = "sides/right end"
  comp_id = 1
  face_set_ids = 1, 3
  type = "flux"
  flux = 0.0
/

&SPECIES_BC
  name = "left end"
  comp_id = 1
  face_set_ids = 2
  type = "mtc"
  mtc = 1.0
  ambient_conc = 2.0
/

### INTERFACE CONDITIONS #######################################################

&THERMAL_BC
  name = "x=0"
  face_set_ids = 10
  type = "interface-htc"
  htc = 1.0
/

&SPECIES_BC
  name = "x=0"
  comp_id = 1
  face_set_ids = 10
  type = "interface-mtc"
  mtc = 1.0
/

### BODIES #####################################################################

&BODY
  surface_name = "from mesh file"
  mesh_material_number = 1
  material_name = "glass"
  temperature = 1.0
  conc = 1.0
/

&BODY
  surface_name = "from mesh file"
  mesh_material_number = 2
  material_name = "glass"
  temperature = 1.0
  conc = 1.0
/
