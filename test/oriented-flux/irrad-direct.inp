#### SIMULATION CONTROL

&MESH
  x_axis%coarse_grid = -1.0, 1.0
  y_axis%coarse_grid = -1.0, 1.0
  z_axis%coarse_grid = -1.0e-5, 1.0e-5
  x_axis%intervals   = 25
  y_axis%intervals   = 50
  z_axis%intervals   = 1
  !rotation_angles = 45., 0., 0.
/

&OUTPUTS
  Output_T  = 0.0, 1.0e-2
  Output_Dt = 100.0
/

&NUMERICS
  dt_constant = 1.0e-3
/

&PHYSICS
  materials = 'stuff'
  heat_transport = .true.
/

&DIFFUSION_SOLVER
  abs_temp_tol = 0.0
  rel_temp_tol = 1.0e-2
  abs_enthalpy_tol = 0.0
  rel_enthalpy_tol = 1.0e-2
  nlk_tol = 0.01
  max_nlk_itr = 5
  pc_amg_cycles = 2
  nlk_preconditioner = 'hypre_amg'
/


#### INITIAL CONDITIONS

&BODY
  surface_name = 'from mesh file'
  mesh_material_number = 1
  material_name = 'stuff'
  temperature = 1.0
/


#### BOUNDARY CONDITIONS & SOURCES

&DED_HEAD
  toolpath = 'laser'
  laser_type = 'gaussian'
  laser_time_const = 0.01e-3
  laser_absorp = 1.0
  laser_power = 1.0 ! [W]
  laser_sigma = 0.4
/

&TOOLPATH
  name = 'laser'
  start_coord = 0.0, 0.0, 0.0
  command_string = '[["setflag",0],["dwell",10.0]]'
/

&VFUNCTION
  name = 'laser'
  type = 'ded-head-laser'
/

&THERMAL_BC
  name = 'laser'
  face_set_ids = 6
  type = 'oriented-flux'
  vflux_func = 'laser'
/

&THERMAL_BC
  name = 'adiabatic-sides'
  face_set_ids = 1, 2, 3, 4, 5
  type = 'flux'
  flux = 0.0
/


#### MATERIALS

&MATERIAL
  name = 'stuff'
  density = 1. ! [kg / m^3]
  specific_heat = 1. ! [J / kg * C]
  conductivity = 0.1 ! [W / m * C]
/
