&MESH

    Mesh_File = 'mesh33.exo'

/

&OUTPUTS

    Output_T                    = 0.0, 40.0
    Output_Dt                   = 1.0
    output_dt_Multiplier    = 1

/

&flow_driver
	new_driver = t
/

&flow_options
	active = t
	viscous_implicitness = 0.5
        inviscid = f
	viscous_number = 0.5
	courant_number = 0.1

/

&flow_predictor
	rel_tol =  1.0e-10,
	abs_tol = 1e-10,
	max_ds_iter = 50,
	max_amg_iter = 5,
	krylov_method = "cg",
/

&flow_corrector
        rel_tol =  1e-10,
	abs_tol = 1e-10,
        max_ds_iter = 10,
	max_amg_iter = 5,
	krylov_method = "cg"
	gmres_krylov_dim = 5
/

&VOLUMETRACKING
	active = f
/


&LINEAR_SOLVER

    name                            = 'projection'
    method                          = 'fgmres'
    preconditioning_method          = 'ssor'
    convergence_criterion           = 1.0e-10
    relaxation_parameter            = 1.4
    preconditioning_steps           = 2
    stopping_criterion              = '||r||',
    maximum_iterations              = 50,

/

&NUMERICS

    volume_track_interfaces     = .false.
    volume_track_brents_method  = .true.
    volume_track_iter_tol       = 1.0e-12
    projection_linear_solution  = 'projection'
    discrete_ops_type           = 'ortho'
    courant_number              = 0.5
    dt_init                     = 1.0e-03
    dt_grow                     = 1.1
    dt_min                      = 1.0e-05
    dt_max                      = 1e-1

/

&PHYSICS

    Body_Force             = 0.0, 0.0, 0.0
    fluid_flow             = .false.
    inviscid               = .false.
    stokes                 = .false.
    surface_tension        = .false.

/

&flow_bc
	! x-, x+, y-
	condition = 'no slip'
	face_sets = 1, 2, 3
/

&flow_bc
	! y+
	condition = 'pressure neumann'
	face_sets = 4
/

&flow_bc
	! y+
	condition = 'velocity dirichlet'
	face_sets = 4
	data = 1.0, 0.0, 0.0
/

&BODY

    Material_number  = 1
    Surface_Name     = 'background'
    Velocity         = 0.0, 0.0, 0.0
    Temperature      = 0.0

/

&MATERIAL

    Material_Name           = 'water'
    Material_Number         = 1
    Material_Feature        = 'background'
    priority                = 1

    Density                 = 1.0

/

&PHASE
  name = 'water'
  property_name(1) = 'density',       property_constant(1) = 1.0
  property_name(2) = 'specific heat', property_constant(2) = 1.0
  property_name(3) = 'viscosity',     property_constant(3) = 0.01
  property_name(4) = 'density deviation', property_constant(4) = 0.0
/

&MATERIAL_SYSTEM
  name = 'water'
  phases = 'water'
/

&PARALLEL_PARAMETERS

    Partitioner  = 'chaco'

/
